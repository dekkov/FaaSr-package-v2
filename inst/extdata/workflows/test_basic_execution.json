{
  "ComputeServers": {
    "My_GitHub_Account": {
      "FaaSType": "GitHubActions",
      "UserName": "test",
      "UseSecretStore": false,
      "ActionRepoName": "FaaSr-workflow",
      "Branch": "main",
      "Token": "My_GitHub_Account_TOKEN"
    }
  },
  "DataStores": {
    "My_Minio_Bucket": {
      "Bucket": "test",
      "Endpoint": "https://play.min.io",
      "Region": "us-east-1",
      "Writable": "TRUE",
      "AccessKey": "My_Minio_Bucket_ACCESS_KEY",
      "SecretKey": "My_Minio_Bucket_SECRET_KEY"
    }
  },
  "ActionList": {
    "upload-file": {
      "FunctionName": "upload",
      "FaaSServer": "My_GitHub_Account",
      "Type": "R",
      "Arguments": {
        "folder": "testing"
      },
      "InvokeNext": [
        "download-file"
      ]
    },
    "download-file": {
      "FunctionName": "download",
      "FaaSServer": "My_GitHub_Account",
      "Type": "R",
      "Arguments": {
        "folder": "testing",
        "file": "test.txt"
      },
      "InvokeNext": [
        "delete-file"
      ]
    },
    "delete-file": {
      "FunctionName": "delete",
      "FaaSServer": "My_GitHub_Account",
      "Type": "R",
      "Arguments": {
        "folder": "testing",
        "file": "test.txt"
      },
      "InvokeNext": []
    }
  },
  "ActionContainers": {
    "upload-file": "ghcr.io/faasr/github-actions-tidyverse",
    "download-file": "ghcr.io/faasr/github-actions-tidyverse",
    "delete-file": "ghcr.io/faasr/github-actions-tidyverse"
  },
  "FunctionGitRepo": {
    "upload": "test/test-repo",
    "download": "test/test-repo",
    "delete": "test/test-repo"
  },
  "FunctionInvoke": "upload-file",
  "InvocationID": "INVOCATION_ID",
  "InvocationIDFromDate": "%Y%m%d%H%M",
  "FaaSrLog": "FaaSrLog",
  "LoggingDataStore": "My_Minio_Bucket",
  "DefaultDataStore": "My_Minio_Bucket",
  "WorkflowName": "test-basic-execution"
}

